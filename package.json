{
  "name": "ai-leadership-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .ts,.tsx,.astro",
    "format": "prettier --write \"**/*.{js,ts,tsx,astro,json,css,md}\"",
